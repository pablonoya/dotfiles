# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/pablo/ml/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/pablo/ml/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/home/pablo/ml/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/home/pablo/ml/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

if [ -n "$1" ]; then
    conda $@
fi
